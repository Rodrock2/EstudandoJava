import java.util.Scanner;

public class Principal{
    public static void main(String[] args) {
        //comparador de numeros inteiros
        Scanner ent = new Scanner(System.in);
        int numero1;
        int numero2;

        System.out.println("Entre com o primeiro numero: ");
        numero1 = ent.nextInt();
        System.out.println("Entre com o segundo numero: ");
        numero2 = ent.nextInt();

        if (numero1 == numero2){
            System.out.printf("O numero %d é igual ao numero %d%n", numero1, numero2);
        }
        if (numero1 != numero2){
            System.out.printf("O numero %d é diferente do numero %d%n", numero1, numero2);
        }
        if (numero1 < numero2){
            System.out.printf("O numero %d é menor que o numero %d%n", numero1, numero2);
        }
        if (numero1 > numero2){
            System.out.printf("O numero %d é maior que o numero %d%n", numero1, numero2);
        }

    }
}
